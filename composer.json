{
  "name": "hexafuchs/psalm-laminas-plugin",
  "description": "A psalm plugin for the Laminas Framework",
  "type": "psalm-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Yasu",
      "email": "yasu@fuchslativ.de"
    }
  ],
  "require": {
    "vimeo/psalm": "dev-master"
  },
  "require-dev": {
    "php-stubs/generator": "^0.8.3",
    "laminas/laminas-authentication": "^2.15",
    "laminas/laminas-db": "^2.18",
    "laminas/laminas-feed": "^2.21",
    "laminas/laminas-filter": "^2.32",
    "laminas/laminas-hydrator": "^4.14",
    "laminas/laminas-i18n": "^2.23",
    "laminas/laminas-mvc": "^3.6",
    "laminas/laminas-mvc-plugin-flashmessenger": "^1.9",
    "laminas/laminas-navigation": "^2.18",
    "laminas/laminas-paginator": "^2.17",
    "laminas/laminas-permissions-acl": "^2.15",
    "laminas/laminas-view": "^2.27",
    "laminas/laminas-form": "^3.13",
    "laminas/laminas-captcha": "^2.16"
  },
  "extra": {
    "psalm": {
      "pluginClass": "Hexafuchs\\PsalmLaminas\\Plugin"
    }
  },
  "autoload": {
    "psr-4": {
      "Hexafuchs\\PsalmLaminas\\": "src"
    }
  },
  "minimum-stability": "dev"
}
